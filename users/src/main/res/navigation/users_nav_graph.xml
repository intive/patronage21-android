<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/users_nav_graph"
    app:startDestination="@id/usersFragment">

    <fragment
        android:id="@+id/usersFragment"
        android:name="com.intive.users.UsersFragment"
        android:label="UsersFragment" >
        <deepLink app:uri="intive://usersFragment" />
        <action
            android:id="@+id/action_usersFragment_to_detailsFragment"
            app:destination="@id/detailsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/detailsFragment2"
        android:name="com.intive.users.DetailsFragment"
        android:label="DetailsFragment" >
        <action
            android:id="@+id/action_detailsFragment2_to_deactivateUserDialogFragment"
            app:destination="@id/deactivateUserDialogFragment" />
    </fragment>
    <dialog
        android:id="@+id/deactivateUserDialogFragment"
        android:name="com.intive.users.DeactivateUserDialogFragment"
        android:label="DeactivateUserDialogFragment" />
</navigation>